{
  "name": "polyglot-v2",
  "version": "2.0.25",
  "description": "UDI Polyglot Version 2",
  "bin": {
    "polyglot-v2": "bin/polyglot-v2.js"
  },
  "main": "bin/polyglot-v2.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node bin/polyglot-v2",
    "start:prod": "NODE_ENV=production npm start",
    "start:dev": "NODE_ENV=development nodemon .",
    "build:armv7": "pkg -t node6-linux-armv7 package.json -o binaries/polyglot-v2-linux-armv7; chmod +x binaries/polyglot-v2-linux-armv7; cd binaries; GZIP=-9 tar cvzf polyglot-v2-linux-armv7.tar.gz polyglot-v2-linux-armv7 --remove-files",
    "build:armv6": "pkg -t node6-linux-armv6 package.json -o binaries/polyglot-v2-linux-armv6; chmod +x binaries/polyglot-v2-linux-armv6; cd binaries; GZIP=-9 tar cvzf polyglot-v2-linux-armv6.tar.gz polyglot-v2-linux-armv6 --remove-files",
    "build:x64": "pkg -t node6-linux-x64 package.json -o binaries/polyglot-v2-linux-x64; chmod +x binaries/polyglot-v2-linux-x64; cd binaries; GZIP=-9 tar cvzf polyglot-v2-linux-x64.tar.gz polyglot-v2-linux-x64 --remove-files",
    "build:x86": "pkg -t node6-linux-x86 package.json -o binaries/polyglot-v2-linux-x86",
    "build:freebsd": "pkg -t node6-freebsd-x64 package.json -o binaries/polyglot-v2-linux-freebsd",
    "build:macos": "pkg -t node6-macos-x64 package.json -o binaries/polyglot-v2-linux-macos"
  },
  "dependencies": {
    "async": "^2.4.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "fs-extra": "^4.0.2",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.0.1",
    "jszip": "^3.1.4",
    "mongoose": "^4.12.3",
    "mosca": "^2.5.2",
    "mqtt": "^=2.13.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "request": "^2.83.0",
    "selfsigned": "^1.10.1",
    "tail": "^1.2.2",
    "websocket-stream": "^5.0.1",
    "winston": "^2.4.0",
    "winston-logrotate": "^1.3.0",
    "ws": "^3.2.0",
    "xml2json-light": "^1.0.4"
  },
  "keywords": [
    "udi",
    "polyglot",
    "nodeserver"
  ],
  "author": "Einstein.42 (milne.james@gmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Einstein42/udi-polyglotv2.git"
  },
  "devDependencies": {
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdoc-route-plugin": "^0.1.0"
  },
  "pkg": {
    "assets": "public/**/*"
  }
}
